    "@typescript-eslint/parser": "7.8.0",
    "@vitest/coverage-v8": "1.6.0",
    "del-cli": "5.1.0",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
