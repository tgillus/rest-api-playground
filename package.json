    "@types/koa": "2.13.8",
    "@types/node": "20.5.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "@vitest/coverage-v8": "0.34.3",
    "del-cli": "5.0.1",
    "eslint": "8.48.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.2",
    "tsx": "3.12.7",
    "typescript": "5.2.2",
    "vitest": "0.34.3"
  },
  "dependencies": {
    "@feathersjs/feathers": "5.0.8",
    "@feathersjs/koa": "5.0.8",
    "@feathersjs/socketio": "5.0.8",
    "@hapi/hapi": "21.3.2",
    "@types/express": "4.17.17",
